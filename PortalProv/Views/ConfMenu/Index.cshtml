@model IEnumerable<Wareways.PortalProv.Infraestructura.GEN_Menu>







<div class="box box-default">
    <div class="box-header">
        <div class="col-md-8">
            <h4 class="box-title">Permisos Menú</h4>
        </div>
        
    </div>

    <div class="box-body">
        <div class="table-responsive">
            <table class="table table-bordered table-condensed" >
                <tr>
                    <th>
                        @Html.DisplayName("Nombre")
                    </th>
                    <th>
                        @Html.DisplayName("Url")
                    </th>
                    <th>
                        @Html.DisplayName("Iconos")
                    </th>
                    <th>
                        @Html.DisplayName("Padre")
                    </th>

                    <th>
                        @Html.DisplayName("Orden")
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Menu_Nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Menu_Url)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Menu_Icons)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Menu_Parent)
                        </td>
                        
                        <td>
                            @Html.DisplayFor(modelItem => item.Menu_Orden)
                        </td>
                        <td>
                            <a href='@Url.Action("Details",new { id =item.Menu_Id })' class="btn btn-sm btn-warning", title="Editar">
                                <span class="fa fa-pencil"></span>
                            </a>

                        </td>
                    </tr>
                }

            </table>
        </div>
        

    </div>
</div>


